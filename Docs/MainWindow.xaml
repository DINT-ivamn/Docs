<Window x:Class="Docs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Docs"
        mc:Ignorable="d"
        Title="Docs" Height="450" Width="800" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    <StackPanel>
        <Button x:Name="NuevoButton" Margin="5" Content="Nuevo documento" Click="NuevoButton_Click"/>
        <Button x:Name="ConfiguracionButton" Margin="5" Content="Configuración" Click="ConfiguracionButton_Click"/>
    </StackPanel>
</Window>
